{
  "name": "qa-webdriverio",
  "version": "1.0.0",
  "description": "Automação de testes com WebdriverIO para QA",
  "license": "MIT",
  "repository": "github:QueroDelivery/webdriverio-multiremote-boilerplate",
  "scripts": {
    "build": "tsc --noEmit",
    "test-android": "wdio run ./configs/wdio.android.conf.ts",
    "test-ios": "wdio run ./configs/wdio.ios.conf.ts",
    "test-android-headless": "wdio run ./configs/wdio.android.conf.ts --headless",
    "test-ios-headless": "wdio run ./configs/wdio.ios.conf.ts --headless",
    "test-ci-local": "wdio run ./configs/wdio.ci-local.conf.ts"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.4",
